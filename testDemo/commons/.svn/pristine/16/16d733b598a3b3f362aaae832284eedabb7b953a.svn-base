package com.codyy.commons.image;

public class ImageConfig {
	
	private String folder;
	private String resourceFolder;
	private String meetFolder;
	private String meettransFolder;

	public String getFolder() {
		return folder;
	}

	public void setFolder(String folder) {
		this.folder = folder;
	}

	public String getResourceFolder() {
		return resourceFolder;
	}

	public void setResourceFolder(String resourceFolder) {
		this.resourceFolder = resourceFolder;
	}

	public String getMeetFolder() {
		return meetFolder;
	}

	public void setMeetFolder(String meetFolder) {
		this.meetFolder = meetFolder;
	}

	public String getMeettransFolder() {
		return meettransFolder;
	}

	public void setMeettransFolder(String meettransFolder) {
		this.meettransFolder = meettransFolder;
	}
	
}
